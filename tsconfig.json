{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions" : {
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@services/*": ["src/services/*"],
      "@assets/*": ["src/assets/*"],
      "@mock/*": ["src/mock/*"],
      "@redux/*": ["src/redux/*"]
    }
  }
}
